<div id="login-container">
    <h1 id="login-header">Login Form</h1>
    <form>
        <div id="username-login">
            <label class="label" for="username">Username</label>
            <input type="username" class="input" placeholder="Enter username here!">
        </div>
        <br>
        <div id="password-signup">
            <label class="label" for="password">Password</label>
            <input type="password" class="input" placeholder="Enter password here!">
        </div>
        <br>
        <button type="submit" id="submit-login">Sign Up</button>
    </form>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="js/login.js"></script>