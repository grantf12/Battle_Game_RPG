<ul id="navbar">
    <li><a href="?" class="nav-links">Home</a></li>
    <li><a href="?" class="nav-links">About Us</a></li>
    <li><a href="?" class="nav-links">Logout</a></li>
</ul>
<div id="character-select-container">
    <h1>Select Your Character</h1>
        {{#each characters}}
        <button onclick=selected("{{name}}") id="{{id}}">{{name}}</button>
        {{/each}}
    <br>
    <h2>Warrior Attack!: <span class="span" id="stat-1">{{characters.Warrior.atk}}</span></h2>
    <h2>Warrior Magical Attack!: <span class="span" id="stat-2">{{magAttack}}</span></h2>
    <h2>Warrior Defense!: <span class="span" id="stat-3">{{defense}}</span></h2>
    <h2>Warrior Magical Defense!: <span class="span" id="stat-4">{{magDefense}}</span></h2>
    <h2>Warrior Health!: <span class="span" id="stat-5">{{health}}</span></h2>
    <input type="submit" id="submit-character-selection" value="Begin Battle">
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="js/character-select.js"></script>
