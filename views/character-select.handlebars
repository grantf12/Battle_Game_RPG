<div id="character-select-container">
    <h1>Select Your Character</h1>
        <label for="characters">Characters</label>
        <div class="character-select">
            <button value="Warrior" onclick=selected("warrior")>Warrior</button>
            <button value="Mage">Mage</button>
            <button value="Tank">Tank</button>
            <button value="Cannon">Cannon</button>
            <button value="Wall">Wall</button>
            <button value="Jack">Jack</button>
        </div>
    <br>
    <h2>Health: {{health}}</h2>
    <h2>Atk: {{attack}}</h2>
    <h2>Def: {{defense}}</h2>
    <h2>Magical Attack: {{magAttack}}</h2>
    <h2>Magical Defence: {{magDefense}}</h2>
    <input type="submit" id="submit-character-selection" value="Begin Battle">
</div>
<link rel="stylesheet" type="text/css" href="assets/style.css" media="screen">
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="js/character-select.js"></script>

