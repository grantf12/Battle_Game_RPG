<ul id="navbar">
    <li><a href="?" class="nav-links">Home</a></li>
    <li><a href="?" class="nav-links">Logout</a></li>
</ul>
<div id="character-select-container">
    <h1>Select Your Character</h1>
        {{#each characters}}
        <button onclick=selected("{{name}}") id="{{id}}">{{name}}</button>
        {{/each}}
    <br>
    <h2>Attack!: <span class="span" id="stat-1"></span></h2>
    <h2>Magical Attack!: <span class="span" id="stat-2"></span></h2>
    <h2>Defense!: <span class="span" id="stat-3"></span></h2>
    <h2>Magical Defense!: <span class="span" id="stat-4"></span></h2>
    <h2>Health!: <span class="span" id="stat-5"></span></h2>
    <input type="submit" onclick="" id="submit-character-selection" value="Begin Battle">
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="js/character-select.js"></script>
