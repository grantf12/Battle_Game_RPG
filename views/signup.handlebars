<div id="signup-container">
    <h1 id="signup-header">Signup Form</h1>
    <form class="signup">
        <div id="username-signup">
            <label class="label" for="username">Email</label>
            <input id="email-input" type="username" class="input" placeholder="example@example.com">
        </div>
        <br>
        <div id="password-signup">
            <label class="label" for="password">Password</label>
            <input id="password-input" type="password" class="input" placeholder="Password">
        </div>
        <br>
        <button type="submit" id="submit-signup">Sign Up</button>
        <p>Once you have signed up you can login    <a href="/login">here</a></p>
    </form>
</div>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript" src="js/signup.js"></script>

</html>